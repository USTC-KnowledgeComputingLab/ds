{
    "name": "ds-bnf",
    "version": "0.1.0",
    "description": "BNF parser and unparsers for DS - conversion between lisp-like and traditional syntax",
    "author": "Hao Zhang <hzhangxyz@outlook.com>",
    "license": "AGPL-3.0-or-later",
    "type": "module",
    "exports": {
        ".": "./dist/index.js"
    },
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "generate": "antlr4 -Dlanguage=JavaScript -visitor -o src/generated ../grammars/Ds.g4 ../grammars/Dsp.g4",
        "build": "npm run generate && tsc",
        "test": "node --test tests/*.test.js",
        "clean": "rm -rf dist src/generated"
    },
    "dependencies": {
        "antlr4": "^4.13.2"
    },
    "devDependencies": {
        "@types/node": "^20.17.9",
        "typescript": "^5.9.3"
    }
}
