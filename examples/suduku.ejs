<%# 数独行列 %>

((Cell `x `y) = `n)
(`x != `x')
---------------------
((Cell `x' `y) != `n)

((Cell `x `y) = `n)
(`y != `y')
---------------------
((Cell `x `y') != `n)

<%
for (let i = 1; i <= 9; i++) {
    for (let j = 1; j <= 9; j++) {
        if (i !== j) {
-%>
    <%- `((Cell \`x ${j}) != \`n)` %>
<%
}
}
-%>
-------------------------------------
<%- `((Cell \`x ${i}) = \`n)` %>

<%
}
%>

<%
for (let i = 1; i <= 9; i++) {
    for (let j = 1; j <= 9; j++) {
        if (i !== j) {
-%>
    <%- `((Cell ${j} \`y) != \`n)` %>
<%
}
}
-%>
-------------------------------------
<%- `((Cell ${i} \`y) = \`n)` %>

<%
}
%>

<%# 数独分块 %>

