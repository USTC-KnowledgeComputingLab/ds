    for (let jb = 1; jb <= 3; jb++) {
        for (let it = 1; it <= 3; it++) {
            for (let jt = 1; jt <= 3; jt++) {
                const xt = (ib - 1) * 3 + it;
                const yt = (jb - 1) * 3 + jt;
                for (let i = 1; i <= 3; i++) {
                    for (let j = 1; j <= 3; j++) {
                        const x = (ib - 1) * 3 + i;
                        const y = (jb - 1) * 3 + j;
                        if (i !== it || j !== jt) {
%>
    <%- `((Cell ${x} ${y}) = \`n)` %>
    ------------------------------------
    <%- `((Cell ${xt} ${yt}) != \`n)` %>
