<%# 数独棋盘 %>
<%
const suduku_data = (
    "  9   37\n" +
    "854397\n" +
    "  3\n" +
    "29      4\n" +
    "   84 5 2\n" +
    " 482 6\n" +
    " 3 4 59\n" +
    "4 2 13  7\n" +
    "   6    3\n" +
    "");

const suduku_array = suduku_data.split("\n").map(row => row.split("").map(cell => cell === " " ? 0 : parseInt(cell)));

for (let i = 0; i < 9; i++) {
    for (let j = 0; j < 9; j++) {
        if (suduku_array[i][j]) {
%>
    <%- `((Cell ${i+1} ${j+1}) = (Literal ${suduku_array[i][j]}))` %>
<%
}
}
}
%>
